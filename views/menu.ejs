<script>
	$(document).ready(function() {

		$().UItoTop({
			easingType : 'easeOutQuart'
		});
		$('.gallery a.gal').touchTouch();
		getCategories();

	});

	function getCategories() {
		var url = '/categories';
		var catId;
		var catName;
		var prodCatURL;

		$.get(url, function(catList) {
			var i;
			var list = '';

			for (i = 0; i < catList.length; ++i) {
				catName = catList[i].category_name;
				catId = catList[i].category_id;
				prodCatURL = '/category/'+catId;
				if (i == 0) {
					list = list
							+ '<li class="selected"><a href="#" class="" onClick="displayItemsByCat(\''+prodCatURL+'\')">'
							+ catName + '</a></li>';
				} else {

					list = list + '<li><a href="#" onClick="displayItemsByCat(\''+prodCatURL+'\')">' + catName
							+ '</a></li>';
				}
			}

			$("#catList").append(list);

		});
	}
	
	function displayItemsByCat(prodCatURL){
		console.log("Inside getMenuItems");
		 $( "#tab-1").empty();   
		
     	var itemId;
     	var itemName;
     	var price;
     	var image;
     	
     	$.get(prodCatURL, function(itemList) 
    	{
            var j;
            var list = '';
           	
			for (j = 0; j < itemList.length; ++j)
			{
			
				itemName = itemList[j].item_name;
				itemId = itemList[j].item_id;
				price = itemList[j].price;
				image = "images/page3_img"+(j+1)+".jpg";
				
    			
    			list = list + '<div class="grid_3"><a href="'+image+'" class="gal"><img src="'+image+'" alt=""><span></span></a>'+
								'<div class="col2"><span class="col3"><a href="#">'+itemName+'</a></span> $ '+price+'</div></div>';
			}
            $( "#tab-1").append(list);        
             
         });
	}
</script>
